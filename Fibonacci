            static void fibonacciseries(int n)
            {
                int num1=0,num2=1, newnum;
                newnum = num1 + num2;
                for (int i = 2; i <n; i++)
                {

                Console.WriteLine(newnum);
                num1 = num2;
                num2 = newnum;
                newnum = num1 + num2;
                


            }
            }

